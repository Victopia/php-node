{
  "http": {
    "output": {
      "buffer": {
        "enable": true,
        "size": 1024
      }
    }
  },

  "auth": {
    "paths": {
      ".private": [ "IsInternal" ],
      "*": true
    },
    "statusCode": 404
  },

  "resolvers": {
    "file": {
      "indexes": "Home index"
    },
    "service": {
      "prefix": "/service"
    },
    "errordocs": {
      "directory": ".private/templates/errordocs"
    }
  }
}
